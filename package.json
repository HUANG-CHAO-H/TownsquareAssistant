{
  "name": "townsquare_assistant",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --config config/vite.config.ts && ts-node config/manifest.ts",
    "preview": "vite preview",

    "build-watch": "tsc && vite build --config config/vite.config.ts --mode development",
    "build-manifest": "ts-node config/manifest.ts"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@douyinfe/semi-ui": "~2.21.2"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.198",
    "@types/node": "^18.11.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "less": "^4.1.3"
  }
}
