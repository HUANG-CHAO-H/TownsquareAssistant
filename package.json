    "lodash": "~4.17.21"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.198",
    "@types/node": "^18.11.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "@types/crypto-js": "~4.1.1",
    "@types/lodash": "~4.14.191",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "less": "^4.1.3"
  }
